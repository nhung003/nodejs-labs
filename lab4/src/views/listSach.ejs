
<% for(let product of list){%>
  <div class="group">
    <a href="/book/<%=product.idProduct%>" >
      <div class="aspect-w-1 aspect-h-1 w-full overflow-hidden rounded-lg bg-gray-200 xl:aspect-w-7 xl:aspect-h-8">
        <img src="/image/<%=product.images%>" alt="Tall slender porcelain bottle with natural clay textured body and cork stopper." class="h-200 w-150 object-cover object-center group-hover:opacity-75">
      </div>
      <h3 class="mt-4 text-center text-gray-700"><%=product.nameProduct%></h3>
      <p class="mt-1 text-center font-medium text-gray-900">$<%=product.priceProduct%></p>
    </a>
  </div>
<%}%>